{
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "bulma-addons": "^0.1.12",
    "bulma-fluent": "^0.3.14",
    "intersection-observer": "^0.5.0",
    "register-service-worker": "^1.5.2",
    "scrollama": "^1.4.4",
    "vue": "^2.5.17",
    "vue-bulma-accordion": "^0.3.5",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-plugin-pwa": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-prettier": "^3.0.4",
    "eslint": "^5.6.1",
    "imports-loader": "^0.8.0",
    "lint-staged": "^7.3.0",
    "node-sass": "^4.9.3",
    "postcss-import": "^12.0.0",
    "sass-loader": "^7.1.0",
    "postcss-url": "^8.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/prettier"
    ],
    "globals": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "root": true,
    "rules": {
      "no-console": 1
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "name": "src",
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "private": true,
  "scripts": {
    "build": "vue-cli-service build --modern",
    "dev": "npm run serve --open",
    "lint": "vue-cli-service lint --fix",
    "serve": "vue-cli-service serve"
  },
  "version": "0.1.0"
}
